{
  "name": "salim-bot",
  "version": "4.2.1500",
  "description": "This bot is Salim. Whenever you trigger it, It become mad.",
  "type": "module",
  "main": "dist/bot.js",
  "private": true,
  "packageManager": "pnpm@8.6.6",
  "scripts": {
    "build": "tsc",
    "start": "node --es-module-specifier-resolution=node dist/bot.js",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint src && prettier --check \"src/**/*.ts\""
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "devDependencies": {
    "@leomotors/scripts": "3.1.0",
    "@trivago/prettier-plugin-sort-imports": "4.1.1",
    "@types/node": "18.16.19",
    "@types/uuid": "9.0.2",
    "@typescript-eslint/eslint-plugin": "6.0.0",
    "@typescript-eslint/parser": "6.0.0",
    "eslint": "8.44.0",
    "prettier": "3.0.0",
    "tsx": "3.12.7",
    "typescript": "5.1.6"
  },
  "dependencies": {
    "@discordjs/opus": "0.9.0",
    "@discordjs/voice": "0.16.0",
    "@prisma/client": "5.0.0",
    "chalk": "5.3.0",
    "cocoa-discord-utils": "2.3.0",
    "discord.js": "14.11.0",
    "ffmpeg-static": "5.1.0",
    "google-tts-api": "2.0.2",
    "libsodium-wrappers": "0.7.11",
    "microsoft-cognitiveservices-speech-sdk": "1.30.1",
    "prisma": "5.0.0",
    "strip-ansi": "7.1.0",
    "uuid": "9.0.0",
    "ytdl-core": "4.11.4"
  },
  "pnpm": {
    "overrides": {
      "@discordjs/voice": "0.16.0"
    }
  }
}
